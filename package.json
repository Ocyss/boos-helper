{
  "name": "boss-helper",
  "displayName": "BossHelper",
  "type": "module",
  "version": "0.3.5",
  "private": true,
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee",
  "description": "Boss直聘助手, 皆在减少投递简历的麻烦, 和提高投递简历的效率,技术栈使用VIte + Vue3 + element-plus",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "postinstall": "wxt prepare",
    "check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "openapi": "openapi-typescript http://localhost:8002/openapi.json -o ./src/types/openapi.d.ts",
    "build": "run-p build:chrome build:firefox build:edge",
    "build:chrome": "wxt build -b chrome",
    "build:firefox": "wxt build -b firefox",
    "build:edge": "wxt build -b edge",
    "zip": "run-p zip:chrome zip:firefox zip:edge",
    "zip:chrome": "wxt zip -b chrome",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge"
  },
  "dependencies": {
    "@types/nanoassert": "^2.0.3",
    "@vueuse/integrations": "^13.0.0",
    "axios": "^1.7.7",
    "element-plus": "^2.9.4",
    "fetch-event-stream": "^0.1.5",
    "mitem": "^1.0.9",
    "nanoassert": "^2.0.0",
    "openai": "^4.85.1",
    "openapi-fetch": "^0.13.5",
    "partial-json": "^0.1.7",
    "qrcode": "^1.5.4",
    "serialize-error": "^12.0.0",
    "vue": "^3.4.38",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@types/chrome": "^0.0.301",
    "@types/node": "^20.16.3",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^13.0.0",
    "@wxt-dev/module-vue": "^1.0.2",
    "eslint": "^9.19.0",
    "openapi-typescript": "^7.6.1",
    "prettier-eslint": "^16.3.0",
    "proto2api": "^0.0.8",
    "protobufjs": "^7.4.0",
    "sass": "^1.77.8",
    "ts-proto": "^1.181.2",
    "typescript": "^5.5.4",
    "uid": "^2.0.2",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-element-plus": "^0.9.0",
    "unplugin-vue-components": "^0.26.0",
    "vue-tsc": "^1.8.27",
    "wxt": "^0.19.26"
  }
}
